{
  "name": "html5-polymer-app-skel",
  "version": "0.0.1",
  "description": "Skeleton for Society Pro material design applications",
  "main": "default.htm",
  "devDependencies": {
    "bower": "^1.3.1",
    "http-server": "^0.6.1",
    "karma": "~0.10",
    "protractor": "~0.20.1",
    "selenium-webdriver": "^2.40.0",
    "shelljs": "^0.2.6",
    "webdriverjs-retry": "^0.2.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000",
    "pretest": "npm install; npm run update-webdriver",
    "test": "protractor test/protractor-conf.js",
    "karma": "karma start test/karma.conf.js",
    "karma-single-run": "karma start test/karma.conf.js  --single-run",
    "update-webdriver": "webdriver-manager update",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + cat('app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CambrianExp/html5-app-skel.git"
  },
  "keywords": [
    "cambrian",
    "SocietyPro"
  ],
  "author": "CambrianExp",
  "license": "MIT",
  "readmeFilename": "README.md"
}
